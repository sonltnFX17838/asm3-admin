{
  "routes": [
    { "src": "/admin(/|$)(.*)", "dest": "/admin/index.html" },
    { "src": "/admin/home(/|$)(.*)", "dest": "/admin/index.html" },
    { "src": "/admin/products(/|$)(.*)", "dest": "/admin/index.html" },
    {
      "src": "/admin/product/:productId(/|$)(.*)",
      "dest": "/admin/index.html"
    },
    { "src": "/admin/new-product(/|$)(.*)", "dest": "/admin/index.html" },
    { "src": "/admin/dash-board(/|$)(.*)", "dest": "/admin/index.html" },
    { "src": "/admin/detail/:idDetail(/|$)(.*)", "dest": "/admin/index.html" },
    { "src": "/chat(/|$)(.*)", "dest": "/chat/index.html" },
    { "src": "/login(/|$)(.*)", "dest": "/login/index.html" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
